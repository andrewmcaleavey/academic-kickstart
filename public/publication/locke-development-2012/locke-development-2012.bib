 issn = {0748-1756},
 journal = {Measurement and Evaluation in Counseling and Development},
 keywords = {alternate forms reliability, clinical assessment, concurrent validity, mental health, test–retest reliability},
 month = {July},
 number = {3},
 pages = {151--169},
 title = {Development and Initial Validation of the Counseling Center Assessment of Psychological Symptoms–34},
